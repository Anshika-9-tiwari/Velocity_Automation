generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user_details {
  id          Int      @id @default(autoincrement())
  first_name  String
  last_name   String
  email_id    String
  phone_no    String
  product_name String
  message     String
  createdAt   DateTime @default(now())
}
